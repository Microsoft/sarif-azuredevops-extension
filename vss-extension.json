{
	"manifestVersion": 1,
	"description": "View Sarif artifacts directly from the build page.",
	"categories": ["Azure Pipelines"],
	"repository": {
		"type": "git",
		"uri": "https://github.com/microsoft/sarif-azuredevops-extension"
	},
	"CustomerQnASupport": {
		"enableqna": true
	},
	"targets": [{ "id": "Microsoft.VisualStudio.Services" }],
	"icons": { "default": "icon.png" },
	"content": {
		"details": { "path": "overview.md" }
	},
	"restrictedTo": [
		"anonymous",
		"public",
		"member"
	],
	"files": [
		{ "addressable": true, "path": "index.html" },
		{ "addressable": true, "path": "bundle.js" }
	],
	"scopes": ["vso.build"]
}
